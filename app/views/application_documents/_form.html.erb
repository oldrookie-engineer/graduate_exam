<%= form_with(model: @application_document, local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>幼稚園
  <%= f.label :title %>
  <%= f.select :title, [[""],["設置認可申請"],["園長選任届"],["位置変更届"],["園地・園舎変更届"],["資産総額変更届"],["廃園届"]] %>
  <%= f.label :application_date %>
  <%= f.date_field :application_date %>
  <%= f.label :processing_deadline %>
  <%= f.date_field :processing_deadline %>
  <%= f.label :processing %>
  <%= f.select :processing, ApplicationDocument.processings.keys.to_a, include_blank: true %>
  <%= f.submit %>
<% end %>
