  <div class="archive_mask"> -->
  <%= form_with(model: @archive, local: true) do |f| %>
    <%if @archive.errors.any?%>
      <div class="error_explanation">
        <h2><%= @archive.errors.count %>件のエラーがあります。</h2>
        <ul>
          <% @archive.errors.full_messages.each do |msg| %>
            <%= msg %>
          <% end %>
        </ul>
      </div>
    <% end %>
    <dl class="kindergarten-archive">
      <dt><%= f.label :name %></dt>
      <dd><%= f.text_field :name %> 幼 稚 園</dd>
      <dt><%= f.label :title %></dt>
      <dd><%= f.select :title, [[""],["設置認可申請"],["園長選任届"],["位置変更届"],["園地・園舎変更届"],["資産総額変更届"],["廃園届"]] %></dd>
      <dt><%= f.label :installation_date %></dt>
      <dd><%= f.date_field :installation_date %></dd>
      <dt><%= f.label :status_of_use %></dt>
      <dd><%= f.select :status_of_use, Archive.status_of_uses.keys.to_a, include_blank: true %></dd>
    </dl>
    <div class="form-submit">
      <%= f.submit %>
    </div>
  <% end %>
